#include "main.h"
/**
 * Main - Entry point
 *
 * Description:  Write a program that prints "-putchar"
 * followed by a new line
 * Do not use standard libraries.
 *
 * Return: 0 if Successful (Always)
 * non-zero otherwise.
 */
int main(void)
{
	char SWMuriu[] = "_putchar";
	int j = 0;

	while (SWMuriu[j] != '\0')
	{
		_putchar(SWMuriu[j]);
		j++;
	}
	_putchar('\n');

	return (0);
}
