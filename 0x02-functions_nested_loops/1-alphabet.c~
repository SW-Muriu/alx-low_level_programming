#include "main.h"
/**
 * main:Entry point
 * Description: print lowercased alphabets,
 * 	Only use _putchar twice in the code
 * Return 0 if Successful (Always)
 */
void print_alphabet(void)
{
	char alphabet = 'a';
	
	while (alphabet <= 'z')
	{
		_putchar(alphabet);
		alphabet++;
	}
	_putchar('\n');

	return (0);
}
